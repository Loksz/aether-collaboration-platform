{
  "name": "@aether/api",
  "version": "0.1.0",
  "private": true,
  "description": "AETHER backend API with Express and WebSocket",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch --env-file=.env src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "db:migrate": "prisma migrate dev",
    "db:generate": "prisma generate",
    "db:seed": "tsx prisma/seed.ts",
    "db:studio": "prisma studio"
  },
  "dependencies": {
    "@aether/types": "workspace:*",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "ioredis": "^5.9.1",
    "jsonwebtoken": "^9.0.2",
    "lib0": "^0.2.117",
    "pg": "^8.16.3",
    "prosemirror-markdown": "^1.13.4",
    "puppeteer": "^24.37.2",
    "socket.io": "^4.8.3",
    "uuid": "^13.0.0",
    "winston": "^3.11.0",
    "ws": "^8.19.0",
    "y-websocket": "^3.0.0",
    "yjs": "^13.6.29",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/ms": "^2.1.0",
    "@types/node": "^20.19.27",
    "@types/pg": "^8.16.0",
    "@types/uuid": "^11.0.0",
    "@types/ws": "^8.18.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}
